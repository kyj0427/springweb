<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="zreview.ReviewMapper">

	<select id='getCateList' resultType=''>
		SELECT * FROM cate
	</select>
	
	<select id='getFoodList' parameterType='' resultType=''>
		SELECT * FROM food
		WHERE  cate_no=#{cate_no}
	</select>
</mapper>
